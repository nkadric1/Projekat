            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <bottom>
      <Button mnemonicParsing="false" onAction="#addnew" prefHeight="38.0" prefWidth="144.0" text="Add new employee" textAlignment="CENTER" BorderPane.alignment="CENTER" />
